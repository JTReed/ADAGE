
%h1=@user.player_name

%h2 contexts 

%script
  != "contexts = #{@contexts.to_json};"
  != "context_names = #{@context_names.to_json};"
%script{src: '/assets/context_graph.js', type: 'text/javascript'}
%div{id: 'context_graph'}
%div{id: 'x_axis'}



-@context_starts.each do |key, value|
  %p= key.to_s + " : " +  value.to_s + " starts"
-@context_ends.each do |key, value|
  %p= key.to_s + " : " +  value.to_s + " ends"
-@context_list.each do |value|
  %p= value


